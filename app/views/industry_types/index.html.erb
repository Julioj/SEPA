<p id="notice"><%= notice %></p>

<h1>Industry Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @industry_types.each do |industry_type| %>
      <tr>
        <td><%= industry_type.name %></td>
        <td><%= link_to 'Show', industry_type %></td>
        <td><%= link_to 'Edit', edit_industry_type_path(industry_type) %></td>
        <td><%= link_to 'Destroy', industry_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Industry Type', new_industry_type_path %>
